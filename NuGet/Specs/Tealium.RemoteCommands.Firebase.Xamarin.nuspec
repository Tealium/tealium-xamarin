    <file src="../../Integrations/Firebase/Tealium.RemoteCommands.Firebase.Droid/bin/Release/Tealium.RemoteCommands.Firebase.Droid.dll" target="lib/MonoAndroid50/Tealium.RemoteCommands.Firebase.Droid.dll" />

