    <file src="../../Integrations/Firebase/Tealium.RemoteCommands.Firebase.Common/bin/Release/Tealium.RemoteCommands.Firebase.Common.dll" target="lib/MonoAndroid50/Tealium.RemoteCommands.Firebase.Common.dll" />

