            <dependency id="Xamarin.Firebase.Analytics" version="[120.0,121.0)"/>
            <dependency id="Tealium.Xamarin" version="[2.0.1,3.0)" />
        </group>
        <group targetFramework="Xamarin.iOS1.0">
